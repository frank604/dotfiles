defaults
auth    on
tls on

account GMAIL
host    smtp.gmail.com
port 587
from (email)@gmail.com
user (email)@gmail.com
password (password)
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account default: GMAIL

account FWS
host smtp.(mailserver).com
port 587
from (email)@(mailserver).com
user (email)@(mailserver).com
password (password)
tls on
tls_starttls on
tls_certcheck off
auth login
